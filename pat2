#include<stdio.h>
void main()
{
    int i,j,x,n=5,count=0;
    for(i=0;i<n;i++)
    {
        for(j=0;j<=(i*2)+1;j++)
        {
            if(j%2==0)
            printf("*");
            else 
            {
                if(i%2==0)
                {
                    printf("%d",++count);
                    x=count+i+2;
                    
                }
                else 
                {
                    printf("%d",x--); 
                    count++;
                }
            }
        }
        printf("\n");
    }
}
